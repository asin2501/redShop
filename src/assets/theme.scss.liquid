/*================ Mixins================*/
//mixins, like breakpoint or other...
@mixin breakpoint($point) {
    @if $point == medium {
        @media (max-width: 1199px) {
            @content;
        }
    } @else if $point == small {
        @media (max-width: 991px) {
            @content;
        }
    } @else if $point == extra-small {
        @media (max-width: 767px) {
            @content;
        }
    } @else if $point == small-tablet-portrait {
        @media (max-width: 575px) {
            @content;
        }
    } @else if $point == mobile-landscape {
        @media (max-width: 480px) {
            @content;
        }
    } @else if $point == mobile {
        @media (max-width: 380px) {
            @content;
        }
    }
}
@mixin placeholder {
    &::-webkit-input-placeholder {
        @content;
    }

    &:-moz-placeholder {
        @content;
    }

    &::-moz-placeholder {
        @content;
    }

    &:-ms-input-placeholder {
        @content;
    }
}



/*================ Base ================*/
// fonts
$book: 'NeuzeitS LT Book', sans-serif;
$book-heavy: 'NeuzeitS LT Book-Heavy', sans-serif;

//colors
$gray: #777777;
$light_gray: #e5e5e5;
$dark_gray: #686868;
$black: #101010;
// Book
@font-face {
    font-family: 'NeuzeitS LT Book';
    src: url('NeuzeitSLT-Book.eot');
    src: url('NeuzeitSLT-Book.eot?#iefix') format('embedded-opentype'),
    url('NeuzeitSLT-Book.woff2') format('woff2'),
    url('NeuzeitSLT-Book.woff') format('woff'),
    url('NeuzeitSLT-Book.ttf') format('truetype'),
    url('NeuzeitSLT-Book.svg#NeuzeitSLT-Book') format('svg');
    font-weight: normal;
    font-style: normal;
}

//Book Heavey
@font-face {
    font-family: 'NeuzeitS LT Book-Heavy';
    src: url('NeuzeitSLT-BookHeavy.eot');
    src: url('NeuzeitSLT-BookHeavy.eot?#iefix') format('embedded-opentype'),
    url('NeuzeitSLT-BookHeavy.woff2') format('woff2'),
    url('NeuzeitSLT-BookHeavy.woff') format('woff'),
    url('NeuzeitSLT-BookHeavy.ttf') format('truetype'),
    url('NeuzeitSLT-BookHeavy.svg#NeuzeitSLT-BookHeavy') format('svg');
    font-weight: normal;
    font-style: normal;
}
//typography: base styles for fonts, p, a, standard h1, h2 ...

body {
    font-family: $book;
    text-transform: uppercase;
    font-size: 10px;
    color: $gray;
}

h1  {
    font-family: $book-heavy;
    font-size: 20px;
    text-transform: uppercase;
    color: $black;
    @include breakpoint(extra-small) {
        font-size: 16px;
    }
}

h2 {
    font-family: $book-heavy;
    font-size: 14px;
    text-transform: uppercase;
    color: $black;
}

h3 {
    font-size: 10px;
    font-family: $book-heavy;
    color: $black;
    text-transform: uppercase;
    letter-spacing: 1.15px;
}

h6 {
    font-size: 10px;
    font-family: $book;
    font-weight: normal;
    letter-spacing: 1.15px;
    color: $black;
}


.main-container {
    max-width: 1600px;
    padding-right:  180px;
    padding-left:180px;
    margin-right:auto;
    margin-left:auto;

    @include breakpoint(medium) {
        padding-left: 0;  // maybe need to change
        padding-right: 0; // maybe need to change
    }

}


