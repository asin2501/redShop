    @media (min-width: 992px) and (max-width: 1400px) {
        flex-wrap: wrap;
    }

    @include breakpoint(extra-small) {
        flex-wrap: wrap;
    }
}

.subscribe-form__input {
    width: 100%;
    flex-grow: 2;
    flex-shrink: 1;
    padding: 11px 14px;
    border-radius: 0;
    box-shadow: 0;
    border: 1px solid $dark_gray;
    font-family: $book;
    font-size: 13px;
    text-transform: none;
    color: $black;
    -webkit-appearance: none;
    transition: .3s all;

    @include placeholder() {
        color: $black;
        opacity: 1;
    }

    &:focus {
        border-color: $black;
    }

    @include breakpoint(extra-small) {
        height: 40px;
    }
}

.subscribe-form__button {
    flex-grow: 1;
    flex-shrink: 0;
    display: inline-block;
    padding: 0 17px;
    border: none;
    background-color: $black;
    box-shadow: none;
    color: #fff;
    font-family: $book;
    font-size: 10px;
    letter-spacing: 1.15px;
    text-transform: uppercase;
    cursor: pointer;
    transition: .3s all;

    &:hover,
    &:focus {
        box-shadow: inset 0 0 0 3px $black, inset 0 0 0 4px white;
    }

    @media (min-width: 992px) and (max-width: 1400px) {
        width: 100%;
        height: 40px;
    }

    @include breakpoint(extra-small) {
        height: 40px;
        width: 100%;
    }
}

.links-list {
    list-style: none!important;
}

.links-list__item {
    + .links-list__item {
        margin-top: 18px;
    }
}

.links-list__link {
    color: $black;
    letter-spacing: 1.15px;
    word-break: break-word;
    &:hover,
    &:focus {
        text-decoration: underline;
    }

    &.active {
        font-family: $book-heavy;
        text-decoration: none;
    }
}
.tab-container {
    position: relative;
    overflow: hidden;
}

.tab {
    position: absolute;
    left: 15px;
    top: 0;
    visibility: hidden;
    opacity: 0;
    transform: translateX(40px);
    transition-property: all;
    transition-duration: .5s;
    transition-delay: 0s;

    &.active {
        position: static;
        top: 0;
        left: 15px;
        visibility: visible;
        opacity: 1;
        transform: translateX(0);
        transition-delay: .5s;
    }

    ul {
        list-style-type: disc;
        padding-left: 13px;

        + * {
            margin-top: 25px;
        }

        // •
    }
}
