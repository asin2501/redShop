.celebrity {
  -webkit-box-flex: 0;
  -ms-flex: 0 0 16.666667%;
  flex: 0 0 16.666667%;
  max-width: 16.666667%;

  position: relative;
  width: 100%;
  min-height: 1px;
  padding-right: 15px;
  padding-left: 15px;

  @media (max-width: 1550px) {
	-webkit-box-flex: 0;
	-ms-flex: 0 0 25%;
	flex: 0 0 25%;
	max-width: 25%;
  }

  @include breakpoint(medium) {

  }

  @include breakpoint(extra-small) {
	-webkit-box-flex: 0;
	-ms-flex: 0 0 33.333%;
	flex: 0 0 33.333%;
	max-width: 33.333%;
  }

  @media (max-width: 550px) {
	-webkit-box-flex: 0;
	-ms-flex: 0 0 50%;
	flex: 0 0 50%;
	max-width: 50%;

	img {
	  width: 100% !important;
	}
  }

  @media (max-width: 380px) {
	-webkit-box-flex: 0;
	-ms-flex: 0 0 auto;
	flex: 0 0 auto;
	max-width: none;
	margin: 0 auto;
	width: auto;

	img {
	  width: 170px !important;
	}
  }
}


// Cart page
.cart-page {
	//padding-top: 135px;
  .container {
	@media (max-width: 1550px) {
	  max-width: 860px;
	}
	
	@media (max-width: 1199px) {
	  max-width: 960px;
	}
  }

  .page-title {
	margin-bottom: 45px;
  }

  .cart-checkout {
	position: relative;
	margin-bottom: 60px;

	&:before {
	  content: '';
	  position: absolute;
	  left: 0;
	  bottom: -30px;
	  width: 100%;
	  height: 1px;
	  background-color: #e5e5e5;
	}
  }

  .cart-checkout__item {
	display: flex;
	justify-content: space-between;
	margin-bottom: 18px;
	letter-spacing: 1px;

	div {
	  display: flex;
	}

	&:first-child {
	  margin-top: -3px;
	}
  }

  .button--cart {
	width: 100%;

	&:hover {
	  background-color: $black;
	  box-shadow: inset 0 0 0 3px $black, inset 0 0 0 4px #fff;
	}
  }

  .update-btn {
	border: none;
	background-color: transparent;
	text-transform: uppercase;
	color: $gray;
	font-size: 10px;
	font-family: $book;
	cursor: pointer;
  }

  .total-price {
	font-family: $book-heavy;
	font-size: 11px;
	color: $black;
  }

  .cart-page__update-wrapper {
	display: flex;
	align-items: center;
	letter-spacing: 1px;
	color: $black;

	input {
	  padding: 0;
	  letter-spacing: 1px;
	  color: $black;
	}

	.dots-left {
	  position: relative;
	  display: block;
	  margin-left: 30px;

	  &:before {
		content: '';
		position: absolute;
		top: 2px;
		left: -15px;
		height: 100%;
		width: 1px;
		background-image: url(dots.png);
		background-repeat: no-repeat;
	  }
	}
  }

  .cart-page__faq {
	position: relative;
	margin-bottom: 60px;

	&:before {
	  content: '';
	  position: absolute;
	  left: 0;
	  bottom: -30px;
	  width: 100%;
	  height: 1px;
	  background-color: #e5e5e5;
	}
  }

  .cart-page__back-btn {
	letter-spacing: 1px;

	.cart-page-back-icon {
	  display: inline-flex;
	  align-items: center;
	  justify-content: center;
	  width: 13px;
	  height: 13px;
	  margin-right: 5px;
	  padding-top: 2px;
	  padding-right: 3px;
	  text-align: center;
	  transform: rotate(90deg);
	  transition: .3s all;

	  svg {
		fill: $black;
	  }
	}
  }

  .faq__item {
	margin-top: 17px;

	&:first-child {
	  margin-top: -5px;
	}
  }
}
.cart-item {
	display: flex;
	margin-bottom: 30px;

  .cart-item__image {
	border: 1px solid #e5e5e5;
	width: 170px;
	height: 255px;
	margin-right: 30px;

	@media (max-width: 500px) {
	  width: 130px;
	  height: 155px;
	  margin-right: 15px;
	}

	a {
	  width: 170px;
	  @media (max-width: 500px) {
		width: auto;
		height: 100%;
	  }
	}

	img {
	  width: 168px;

	  @media (max-width: 500px) {
		width: auto;
		object-fit: contain;
		height: 153px;
	  }
	}
  }

  .cart-item-content {
	display: flex;
	flex-direction: column;
	width: 100%;
  }

  .cart-item-head {
	display: flex;
	justify-content: space-between;
	width: 100%;
	margin-bottom: 35px;

	a {
	  letter-spacing: 1px;
	}
  }

  .cart-item__price {
	color: $black;
  }

  .cart-item-body {
	display: flex;
	justify-content: space-between;
  }

  .cart-item-group {
	display: flex;
	color: #000;

	.cart-item-group__title {
	  width: 100px;
	  letter-spacing: 1px;
	  font-family: $book-heavy;
	  color: $black;

	  @include breakpoint(mobile) {
		width: 75px;
	  }
	}

	&:not(:last-child) {
	  margin-bottom: 13px;
	}
  }

  .cart-item__remove-btn{
	letter-spacing: 1px;
  }

  .qty__minus,
  .qty__plus {
	color: $black;
	font-size: 13px;
	cursor: pointer;
	user-select: none;
  }

  .qty__input {
	width: 26px;
	text-align: center;
	border: none;
	user-select: none;
	font-size: 11px;
  }

  .qty__input::-webkit-outer-spin-button,
  .qty__input::-webkit-inner-spin-button {
	-webkit-appearance: none;
	margin: 0;
  }
}

.charity-page {

    .container {
        @media (max-width: 1550px) {
            max-width: 860px;
        }
    }

    .page-title {
        margin-bottom: 39px;
    }

    .page-description {
        margin-bottom: 43px;
        letter-spacing: 1px;
        line-height: 2.5;
        max-width: 870px;
        word-spacing: 1px;
    }
}
.charity-item {
  height: 500px;
  padding: 15px;

  @media (max-width: 1550px) {
	height: 400px;
  }

  @media (max-width: 991px) {
	height: 500px;
  }

  @media (max-width: 420px) {
	height: 400px;
  }

  a {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
	padding: 15px;
	background-repeat: no-repeat;
	background-position: 50% 50%;
	background-size: cover;
	filter: grayscale(100%);
	font-family: $book-heavy;
	font-size: 11px;
	letter-spacing: 1px;
	color: #fff;
	transition: filter 0.2s linear;

	&:hover {
	  filter: grayscale(0%);
	}
  }
}

.events-page {
  padding-top: 13px;

  .container {
	@media (max-width: 1550px) {
	  max-width: 860px;
	}

	@media (max-width: 1199px) {
	  max-width: 960px;
	}
  }

  .events-page__title {
	margin-bottom: 60px;
	padding-top: 55px;
	text-align: center;
	border-top: 1px solid #e5e5e5;
	font-family: $book-heavy;
	font-size: 11.5px;
	letter-spacing: 1px;
  }

  .events-page__block {

	&:not(:last-child) {
	  margin-bottom: 50px;
	}
  }

  .load-more {
	margin: 0 auto;

	&:hover {
	  background-color: $black;
	  box-shadow: inset 0 0 0 3px $black, inset 0 0 0 4px #fff;
	}
  }
}
.event-item {
  position: relative;
  max-width: 470px;
  padding-bottom: 73px;
  text-align: center;
  margin: 0 auto;

  .event-item__head {
	margin-bottom: 9px;
	color: $black;
	letter-spacing: 1px;

	span {
	  margin: 0px 8px;
	}
  }

  .button {
	position: absolute;
	left: 50%;
	transform: translateX(-50%);

	&:hover {
	  background-color: $black;
	  box-shadow: inset 0 0 0 3px $black, inset 0 0 0 4px #fff;
	}

	@include breakpoint(small) {
	  position: relative;
	  left: auto;
	  transform: none;
	}
  }

  .event-item__description {
	letter-spacing: 1px;
	line-height: 2.5;
	max-width: 870px;
	word-spacing: 1px;

	@include breakpoint(small) {
	  margin-bottom: 25px;
	}
  }

  h3 {
	margin-bottom: 18px;
	font-family: $book-heavy;
	letter-spacing: 0;
  }

  @include breakpoint(small) {
	margin-bottom: 30px;
  }
}

.event-item--upcoming {

  h3 {
	font-size: 20px;
  }

  .button {
	bottom: 10px;

	@include breakpoint(small) {
	  bottom: auto;
	}
  }

  @include breakpoint(small) {

	&:last-child {
	 // margin-bottom: 0;
	  padding-bottom: 0;
	}
  }
}

.event-item--past {
  padding-bottom: 54px;

  h3 {
	font-size: 11px;
	letter-spacing: 1px;
  }

  .button-arrow {
	display: block;
	width: 30px;
	height: 9px;
	margin: 0 auto;
	margin-top: 10px;
	font-size: 0;
	background-image: url(more.png);
	background-repeat: no-repeat;
	background-size: contain;
  }

  @include breakpoint(small) {
	padding-bottom: 0;
	margin-bottom: 35px;

	&:last-child {
	  //margin-bottom: 0;
	}
  }
}

.js-past-events {
  .js-event-item {
	@include breakpoint(small) {
	  &:last-child {
		.event-item--past {
		  margin-bottom: 0;
		}
	  }
	}
  }
}

